		<div class="main-panel">
			<div class="container rounded bg-white mt-5 mb-5">
				<div class="row">
						<div class="col-md-12 border-right">
							<form action="http://localhost:3000/admin/admin" method="post">
                                {{#each getNews}}
								<div class="row mt-3">
									<div class="col-md-12">
										<label class="labels">ID</label>
										<input type="text" name="idnews" class="form-control" value="{{this._id}}"/>
									</div>
								</div>
								<div class="row mt-3">
									<div class="col-md-12">
										<label class="labels">Title</label>
										<input type="text" name="title" class="form-control" value="{{this.title}}"/>
									</div>
								</div>
                                
								<div class="row mt-12">
									<div class="col-md-12">
                                        <label for="">Content</label>
                                        <textarea class="form-control" name="content" id="" rows="8">{{this.content}}</textarea>
									</div>
								</div>
					
								<div class="row mt-3">
									<div class="col-md-12">
										<label class="labels">Images</label>
										<input type="text" value="{{this.images}}" name="images" class="form-control"  placeholder="Image"/>
									</div>
								</div>
								<div class="row mt-3">
                                    
									<div class="col-md-6">
										<label class="labels">Date</label>
										<input type="text" name="date" class="form-control" placeholder="Both" value="{{this.date}}"/>
									</div>
                                {{/each}}  
									<div class="col-md-6">
										<div class="form-group">
										  <label for="">Author</label>
										  <select class="form-control" name="author" id="">
												{{#each getAuthors}}
													<option >{{this.username}}</option>
												{{/each}}
										  </select>
										</div>
									</div>
								</div>
								<hr>
								<button type="submit" class="btn btn-primary">Submit</button>
							</form>
						</div>
				</div>
			</div>
		</div>
		<script src="/javascripts/profile.js"></script>
