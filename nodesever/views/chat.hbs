        <div class="main-panel">
            <div class="container rounded bg-white mt-5 mb-5">
				<div class="row">
                    {{#findChats}}
                    <div id="chat-content-commemt" style="margin-top:50px;">
                        {{#with this.content}}
                        <div class="row">
                            <div class="content-post-chat">
                                <img src="{{this.[0].images_post}}" alt="" srcset="">
                                <span class="post-chat">
                                    {{this.[0].content}}
                                </span>
                            </div>
                        </div>
                        {{/with}}
                    </div>
                    {{/findChats}}
                    {{#getMoto}}
                        <form style="width:100% ;" action="http://localhost:3000/moto/{{this._id}}/chat" method="post">
                    {{/getMoto}}
                        <div class="value-input">
                        {{#getMoto}}
                            <input type="text" class="form-control" name="nguoiNhan" id="nguoiNhan" aria-describedby="helpId" value="{{this.nguoiBan}}">
                        {{/getMoto}}
                        {{#findChats}}
                            <input type="text" class="form-control" name="id_cu" aria-describedby="helpId" value="{{this._id}}">
                        {{/findChats}}
                        {{#userProfile}}
                            <input type="text" class="form-control" name="images_post" id="nguoiNhan" aria-describedby="helpId" value="{{this.images}}">
                        {{/userProfile}}
                        </div>
                        <div class="submit-content">
                            <div class="form-group">
                                <input type="text" class="form-control" name="valuecontent" id="input-chat" placeholder="">
                            </div>
                            <button type="submit" id="submit-button" class="btn btn-primary">Submit</button>
                        </div>  
                        
                    </form>          
				</div>
			</div>
